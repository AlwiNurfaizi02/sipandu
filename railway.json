{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "paketo",
    "buildCommand": "composer install --optimize-autoloader --no-dev --no-interaction"
  },
  "start": "php artisan serve --host=0.0.0.0 --port=${PORT}",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "PHP_VERSION": "8.3"
  }
}